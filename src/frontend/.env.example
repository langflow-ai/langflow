# AI Studio Frontend Environment Variables
# These will be injected at runtime via env.sh script
# Set these environment variables when running the container

# Backend API Configuration (Required)
VITE_BACKEND_URL=http://localhost:7860
VITE_API_PREFIX=/api/v1

# Proxy Configuration (Development)
VITE_PROXY_TARGET=http://localhost:7860
VITE_PORT=3000

# Application Configuration
VITE_APP_TITLE=AI Studio
VITE_BUILD_VERSION=development

# Feature Flags
VITE_ENABLE_CHAT=true
VITE_ENABLE_AGENT_BUILDER=true
VITE_ENABLE_HEALTHCARE_COMPONENTS=true

# Development/Debug Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# Optional Advanced Configuration
VITE_WEBSOCKET_URL=
VITE_MAX_FILE_SIZE=
VITE_TIMEOUT=

# Keycloak Authentication Configuration
VITE_KEYCLOAK_ENABLED=false
VITE_KEYCLOAK_URL=http://localhost:8080
VITE_KEYCLOAK_REALM=ai-studio
VITE_KEYCLOAK_CLIENT_ID=ai-studio-frontend

# External Prompts URL
VITE_PROMPTS_URL=http://localhost:8174
