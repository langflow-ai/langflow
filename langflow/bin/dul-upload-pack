import os
import sys

from dulwich.porcelain import upload_pack

if len(sys.argv) < 2:
    sys.stderr.write("usage: %s <git-dir>\n" % os.path.basename(sys.argv[0]))
    sys.exit(1)

sys.exit(upload_pack(sys.argv[1]))
